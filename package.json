{
  "name": "ambatsador",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "node server",
    "dev:client": "vite",
    "dev:server": "nodemon server",
    "preview": "npm run build:client && NODE_ENV='staging' npm start",
    "build": "npm run build:client",
    "build:client": "vite build",
    "build:server": "npm run db:migrate",
    "db:migrate": "npm run knex migrate:latest",
    "db:migrate:make": "npm run knex migrate:make -- -x mjs",
    "db:seed": "npm run knex seed:run",
    "db:seed:make": "npm run knex seed:make -- -x mjs",
    "db:reset": "rm -f server/db/dev.sqlite3 && npm run db:migrate && npm run db:seed",
    "knex": "knex --knexfile server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx .",
    "format": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run"
  },
  "dependencies": {
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-jwt": "^6.1.0",
    "jest": "^29.3.1",
    "jwks-rsa": "^2.1.4",
    "knex": "^2.1.0",
    "sqlite3": "^5.1.2",
    "superagent": "^8.0.3"
  },
  "devDependencies": {
    "@auth0/auth0-react": "^1.12.0",
    "@reduxjs/toolkit": "^1.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.23.4",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "jsdom": "^20.0.0",
    "nock": "^13.2.9",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "supertest": "^6.3.1",
    "vite": "^3.2.3",
    "vitest": "^0.25.1"
  }
}
